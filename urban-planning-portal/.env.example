# Server Configuration
NEXT_PUBLIC_API_URL=http://localhost:3001
NEXT_PUBLIC_APP_URL=http://localhost:3000

# Authentication
NEXT_PUBLIC_JWT_COOKIE_NAME=urban_planning_token
NEXT_PUBLIC_AUTH_ENABLED=true

# ArcGIS Configuration
NEXT_PUBLIC_ARCGIS_EPI_URL=your_arcgis_epi_url
NEXT_PUBLIC_ARCGIS_PROTECTION_URL=your_arcgis_protection_url
NEXT_PUBLIC_ARCGIS_LOCAL_PROVISIONS_URL=your_arcgis_local_provisions_url

# Feature Flags
NEXT_PUBLIC_ENABLE_MAPS=true
NEXT_PUBLIC_ENABLE_FILE_UPLOAD=true
NEXT_PUBLIC_ENABLE_NOTIFICATIONS=true
NEXT_PUBLIC_ENABLE_ANALYTICS=false

# UI Configuration
NEXT_PUBLIC_THEME=light
NEXT_PUBLIC_DEFAULT_LOCALE=en
NEXT_PUBLIC_DATE_FORMAT=DD/MM/YYYY

# Performance
NEXT_PUBLIC_ENABLE_CACHE=true
NEXT_PUBLIC_CACHE_TTL=3600

# Error Tracking
NEXT_PUBLIC_ERROR_TRACKING_ENABLED=false
NEXT_PUBLIC_ERROR_TRACKING_DSN=

# SEO
NEXT_PUBLIC_SITE_NAME="Urban Planning Portal"
NEXT_PUBLIC_SITE_DESCRIPTION="Professional portal for urban planning assessments and resources" 