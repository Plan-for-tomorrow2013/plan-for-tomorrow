{
  "name": "urban-planning-professionals-portal",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "npm-run-all --parallel dev:*",
    "dev:admin": "cd admin && npm run dev",
    "dev:portal": "cd urban-planning-portal && npm run dev",
    "build": "npm-run-all --parallel build:*",
    "build:admin": "cd admin && npm run build",
    "build:portal": "cd urban-planning-portal && npm run build",
    "start": "npm-run-all --parallel start:*",
    "start:admin": "cd admin && npm run start",
    "start:portal": "cd urban-planning-portal && npm run start",
    "lint": "npm-run-all --parallel lint:*",
    "lint:admin": "cd admin && npm run lint",
    "lint:portal": "cd urban-planning-portal && npm run lint",
    "format": "prettier --write \"**/*.{js,jsx,ts,tsx,json,md}\"",
    "test": "npm-run-all --parallel test:*",
    "test:admin": "cd admin && jest --passWithNoTests",
    "test:portal": "cd urban-planning-portal && jest --passWithNoTests",
    "test:watch": "npm-run-all --parallel test:watch:*",
    "test:watch:admin": "cd admin && jest --watch --passWithNoTests",
    "test:watch:portal": "cd urban-planning-portal && jest --watch --passWithNoTests",
    "test:coverage": "npm-run-all --parallel test:coverage:*",
    "test:coverage:admin": "cd admin && jest --coverage --passWithNoTests",
    "test:coverage:portal": "cd urban-planning-portal && jest --coverage --passWithNoTests",
    "analyze": "npm-run-all --parallel analyze:*",
    "analyze:admin": "cd admin && npm run analyze",
    "analyze:portal": "cd urban-planning-portal && npm run analyze",
    "type-check": "npm-run-all --parallel type-check:*",
    "type-check:admin": "cd admin && npm run type-check",
    "type-check:portal": "cd urban-planning-portal && npm run type-check",
    "prepare": "husky install",
    "prisma:generate": "prisma generate",
    "prisma:migrate": "prisma migrate dev",
    "prisma:studio": "prisma studio",
    "prisma:deploy": "prisma migrate deploy"
  },
  "devDependencies": {
    "@next/bundle-analyzer": "^14.1.0",
    "@testing-library/jest-dom": "^6.4.2",
    "@testing-library/react": "^14.2.1",
    "@testing-library/user-event": "^14.5.2",
    "@types/jest": "^29.5.12",
    "@types/node": "^20.11.19",
    "@types/react": "^18.2.57",
    "@typescript-eslint/eslint-plugin": "^7.0.2",
    "@typescript-eslint/parser": "^7.0.2",
    "cross-env": "^7.0.3",
    "dotenv-cli": "^7.4.1",
    "eslint": "^8.56.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-prettier": "^5.1.3",
    "eslint-plugin-react": "^7.33.2",
    "eslint-plugin-react-hooks": "^4.6.0",
    "husky": "^9.0.11",
    "jest": "^29.7.0",
    "jest-environment-jsdom": "^29.7.0",
    "lint-staged": "^15.2.2",
    "npm-run-all": "^4.1.5",
    "prettier": "^3.2.5",
    "prisma": "^5.10.0",
    "typescript": "^5.3.3"
  },
  "dependencies": {
    "@prisma/client": "^6.5.0"
  }
}
